e":1,"column":2226}},{"source":"utils\\addRecipes.js","name":"style","original":{"line":74,"column":119},"generated":{"line":1,"column":2240}},{"source":"utils\\addRecipes.js","name":"style","original":{"line":74,"column":119},"generated":{"line":1,"column":2242}},{"source":"utils\\addRecipes.js","name":"display","original":{"line":74,"column":125},"generated":{"line":1,"column":2248}},{"source":"utils\\addRecipes.js","original":{"line":74,"column":133},"generated":{"line":1,"column":2256}},{"source":"utils\\addRecipes.js","name":"ingrDupl","original":{"line":77,"column":16},"generated":{"line":1,"column":2266}},{"source":"utils\\addRecipes.js","name":"ingrDupl","original":{"line":77,"column":16},"generated":{"line":1,"column":2268}},{"source":"utils\\addRecipes.js","original":{"line":77,"column":26},"generated":{"line":1,"column":2270}},{"source":"utils\\addRecipes.js","name":"newList","original":{"line":77,"column":48},"generated":{"line":1,"column":2290}},{"source":"utils\\addRecipes.js","name":"appDupl","original":{"line":77,"column":56},"generated":{"line":1,"column":2292}},{"source":"utils\\addRecipes.js","original":{"line":77,"column":65},"generated":{"line":1,"column":2294}},{"source":"utils\\addRecipes.js","name":"newList","original":{"line":77,"column":83},"generated":{"line":1,"column":2311}},{"source":"utils\\addRecipes.js","name":"ustDupl","original":{"line":77,"column":91},"generated":{"line":1,"column":2313}},{"source":"utils\\addRecipes.js","original":{"line":77,"column":100},"generated":{"line":1,"column":2315}},{"source":"utils\\addRecipes.js","original":{"line":79,"column":8},"generated":{"line":1,"column":2336}},{"source":"utils\\addRecipes.js","original":{"line":79,"column":8},"generated":{"line":1,"column":2338}},{"source":"utils\\addRecipes.js","original":{"line":79,"column":8},"generated":{"line":1,"column":2340}}],"sources":{"utils\\addRecipes.js":"import { Ingredient } from \"../components/Ingredient\";\r\nimport filterFunction from \"./filters\";\r\nimport { filtersArray } from \"./filters\";\r\n\r\nexport default function addRecipes (recipes){\r\n        const htmlString = recipes\r\n            .map((recipe) => {\r\n                return `\r\n                <article class=\"recipe__card\" id=\"recipe-${recipe.id}\">\r\n                <div class=\"recipe__image\">\r\n                    <img src=\"\" alt=\"\">\r\n                </div>\r\n                <div class=\"recipe__info\">\r\n                    <div class=\"recipe__title\">\r\n                        <h2>${recipe.name}</h2>\r\n                        <div class=\"recipe__time\">\r\n                            <img src=\"\" alt=\"\">\r\n                            <span>${recipe.time} min.</span>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"recipe__meta\">\r\n                        <div class=\"ingredients\">\r\n                        <ul id=\"${recipe.id}\">\r\n                        </ul>\r\n                        </div>\r\n                        <div class=\"instructions\">\r\n                            <p>${recipe.description}</p>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </article>\r\n            `;\r\n            })\r\n            .join('');\r\n            document.getElementById('search-results').innerHTML = htmlString;\r\n\r\n\r\n\r\n        //Display the ingredients for each recipes found\r\n        for(let i = 0; i < recipes.length; i++){\r\n            let ingredientsBlock = document.getElementById(recipes[i].id);\r\n            for(let j = 0; j < recipes[i].ingredients.length; j ++) {\r\n                let ingredientsList = new Ingredient(recipes[i].ingredients[j]);\r\n                ingredientsBlock.innerHTML += ingredientsList.displayIngredient();\r\n            }\r\n        } \r\n\r\n        let ingrList = [] ;\r\n        let appList = [];\r\n        let ustList = [];\r\n\r\n        //Récupère les listes mises à jour\r\n        for(let i = 0; i < recipes.length; i++) { \r\n            appList.push(recipes[i].appliance)\r\n            for(let j = 0; j < recipes[i].ingredients.length; j++) { ingrList.push(recipes[i].ingredients[j].ingredient);}\r\n            for(let k = 0; k < recipes[i].ustensils.length; k++) { ustList.push(recipes[i].ustensils[k]) }\r\n        }\r\n        let removeDupl = (list) => {\r\n            return list.filter(function(elem, index, self) {   return index === self.indexOf(elem); })\r\n        }\r\n        let ingrDupl = removeDupl(ingrList); let appDupl = removeDupl(appList); let ustDupl = removeDupl(ustList);\r\n\r\n        //Supprime les anciennes listes\r\n        let oldLst = document.querySelectorAll('.dropdown-menu  span');\r\n        for(let list of oldLst) { list.style.display = \"none\"} \r\n\r\n        //Réinjecte les listes\r\n        let newList = (list, parentId) => {\r\n            for(let i = 0; i < 30; i++) {\r\n                let parentBlock = document.getElementById(parentId);\r\n                let spanWrapper = document.createElement('span');\r\n                parentBlock.appendChild(spanWrapper);\r\n                spanWrapper.innerText += list[i];\r\n                if(spanWrapper.innerText == \"undefined\" || filtersArray.includes(spanWrapper.innerText)) { spanWrapper.style.display=\"none\"}\r\n            }\r\n        }\r\n        newList(ingrDupl, 'ingredients-list') ; newList(appDupl, 'appareils-list', newList(ustDupl, 'ustensiles-list'));\r\n        //Ré-applique la fonction de filtre\r\n        filterFunction();\r\n\r\n}"},"lineCount":null}},"hash":"e0a86c942a0c8b2ebf3b872a26850fc9","cacheData":{"env":{}},"id":28}