dow: none;\r\n}\r\n.dropdown-menu { border: none; color: #ffff; transform: translate3d(0px, 65px, 0px)!important; border-radius: 0px 0px 5px 5px;}\r\n.dropdown-toggle, .dropdown-menu { width: 170px;}\r\n\r\nbody { padding: 0 1em; box-sizing: border-box;}\r\n\r\ntextarea:focus, input:focus{\r\n    outline: none;\r\n}\r\n\r\nul { list-style: none; padding-left: 0;}\r\n\r\nheader {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    align-items: center;\r\n    margin: 50px 0px;\r\n}\r\n\r\nh1 {\r\n    font-family: DM Sans;\r\n    font-style: normal;\r\n    font-weight: bold;\r\n    font-size: 36px;\r\n    line-height: 47px;\r\n    text-align: center;\r\n    color: #D04F4F;\r\n}\r\n\r\n.search__input {\r\n    height: 69px;\r\n    background-color: #E7E7E7;\r\n    border: none;\r\n}\r\n\r\n.dropdown-toggle input { width: 85%; background-color: transparent; border: none; color: #ffff}\r\n.dropdown-toggle input::placeholder { color: #ffff; opacity: 1;}\r\n.onclick::placeholder {opacity: 0.5!important;}\r\n.dropdown-toggle input::focus-visible {outline:0px!important; box-shadow: none !important;}\r\n.search__input:focus { background-color: #E7E7E7;}\r\n\r\n.changeSize { width: 400px;}\r\n\r\n.search__button {\r\n    width: 69px;\r\n    background-color: #E7E7E7;\r\n    border: none;\r\n    padding: 0;\r\n}\r\n\r\n.search__button svg { height: 32px; }\r\n.btn-outline-secondary:hover { background-color: #E7E7E7; color: #000000;}\r\n\r\n\r\n#search-results { \r\n    display: grid;\r\n    grid-template-columns: repeat(3, 380px);\r\n    grid-template-rows: auto;\r\n    justify-content: space-between;\r\n    margin: 30px 0px;\r\n}\r\n\r\narticle {\r\n    height: 385px;\r\n    width: 400px;\r\n    background-color: #E7E7E7;\r\n    border-radius: 5px;\r\n    margin: 20px 0px\r\n}\r\n\r\narticle .recipe__image { height: 160px; width: 100%; background-color: #C7BEBE; border-radius: 5px 5px 0px 0px;}\r\n\r\n.recipe__info { padding: 1em; box-sizing: border-box;}\r\n\r\n.recipe__title { display: flex; justify-content: space-between; align-items: center;}\r\n.recipe__title h2, .recipe__title span { \r\n    font-family: Lato;\r\n    font-size: 18px;\r\n    line-height: 22px;\r\n    font-weight: 400;\r\n    margin: 0;\r\n}\r\n\r\n.recipe__time { display: flex; align-items: center; min-width: 70px;}\r\n.recipe__time img { margin : 0px 5px}\r\n\r\n.recipe__meta { display: flex; font-family: Lato; font-size: 12px; margin-top: 20px;}\r\n.recipe__meta > div { width: 50%;}\r\n.recipe__meta p { \r\n    max-height: 100%; \r\n    line-height: 12px; \r\n    overflow: hidden; \r\n    text-overflow: ellipsis; \r\n    overflow: hidden;\r\n    display: -webkit-box;\r\n    -webkit-line-clamp: 9;\r\n    -webkit-box-orient: vertical; }"},"lineCount":null}},"error":null,"hash":"8b84b4796008ca80b4e017377d2c35d8","cacheData":{"env":{}}}